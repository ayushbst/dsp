public class MyClass {
    
    Node head;
    static class Node
    {
        int n;
        Node next;
        
        Node(int d)
        {
            n=d;
            next=null;
        }
    }
    
    public static void main(String args[]) {
     MyClass list=new MyClass();
     list.head=new Node(1);
     Node second=new Node(2);
     Node third=new Node(3);
     
     list.head.next=second;
     second.next=third;
     
     list.printlist();
     
    list.deleteNode(2);
     
     list.printlist();
    }
    
    void printlist()
    {
        Node x=head;
    
        while(x!=null)
        {
            System.out.println(x.n);
            x=x.next;
        }
    }
    
    void deleteNode(int key)
    {
        Node x=head;
        Node prev=null;
        while(x.n!=key)
        {
            prev=x;
            x=x.next;
        }
        prev.next=x.next;
    }
}

