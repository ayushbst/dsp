public class MyClass {
    
    Node head;
    static class Node
    {
        int n;
        Node next;
        
        Node(int d)
        {
            n=d;
            next=null;
        }
    }
    
    public static void main(String args[]) {
     MyClass list=new MyClass();
     list.head=new Node(1);
     Node second=new Node(2);
     Node third=new Node(3);
     
     list.head.next=second;
     second.next=third;
     
     list.printlist();
     
     list.head=list.reverseList();
     
     list.printlist();
    }
    
    void printlist()
    {
        Node x=head;
    
        while(x!=null)
        {
            System.out.println(x.n);
            x=x.next;
        }
    }
    
    Node reverseList()
    {
        Node prev=null;
        Node curr=head;
        Node next=null;
        
        while(curr!=null)
        {
           next=curr.next;
           curr.next=prev;
           prev=curr;
           curr=next;
        }
        return prev;
    }
    
}
